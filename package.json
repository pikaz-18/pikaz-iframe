{
  "name": "pikaz-iframe",
  "version": "0.1.7",
  "description": "A Iframe Component for Vue.js",
  "private": false,
  "main": "lib/pikaz-iframe.umd.min.js",
  "keywords": [
    "iframe",
    "vue",
    "components"
  ],
  "files": [
    "lib",
    "example"
  ],
  "license": "MIT",
  "author": "pikaz",
  "scripts": {
    "serve": "vue-cli-service serve src/example/main.js",
    "example": "vue-cli-service build --dest example src/example/main.js",
    "lib": "vue-cli-service build --target lib --name pikaz-iframe --dest lib src/plugin/index.js",
    "build": "vue-cli-service build --target lib --name pikaz-iframe --dest lib src/plugin/index.js && vue-cli-service build --dest example src/example/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikaz-18/pikaz-iframe.git"
  },
  "bugs": {
    "url": "https://github.com/pikaz-18/pikaz-iframe/issues"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}